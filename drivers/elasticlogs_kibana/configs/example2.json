{
  "jobs": [
    {
      "job_id": "KibanaExample",
      "concurrency": 1,
      "driver": "elasticlogs_kibana",
      "rate_limit": 0.2,
      "parameters": {
        "index_list": ["rankin_elasticlogs*"],
        "days": "2016-01-28,2016-01-31",
        "interval_days": 28.0,
        "timeout": 60000,
        "text_filter_file":"./drivers/elasticlogs_kibana/configs/filters.txt",
        "use_text_filter":true
      },
      "operations": [
        {
          "name": "traffic",
          "weight": 3,
          "sla": 2000,
          "parameters": {
            "timeout": 30000,
            "interval_days": 0.5,
            "date_histogram_interval": "30m"
          }
        },
        {
          "name": "content_issues",
          "weight": 1,
          "sla": 1000
        }
      ]
    }
  ]
}