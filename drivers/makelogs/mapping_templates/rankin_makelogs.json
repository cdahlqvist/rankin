{
    "template" : "rankin*",
    "settings" : {
      "number_of_shards":4,
      "number_of_replicas":0,
      "index.refresh_interval" : "10s"
    },
    "mappings" : {
      "_default_" : {
        "dynamic_templates" : [ {
          "string_fields" : {
            "mapping" : {
              "index" : "not_analyzed",
              "ignore_above" : 256,
              "type" : "string",
              "doc_values": true,
              "omit_norms" : true
            },
            "match_mapping_type" : "string",
            "match" : "*"
          }
        },
        {
          "numeric_fields" : {
            "mapping" : {
              "type" : "long",
              "doc_values": true
            },
            "match_mapping_type" : "long",
            "match" : "*"
          }
        } ],
        "properties" : {
          "@version" : {
            "index" : "not_analyzed",
            "type" : "string",

            "omit_norms" : true
          },
          "@timestamp" : {
            "format" : "dateOptionalTime",
            "type" : "date",
            "doc_values": true
          },
          "geo": {
            "properties": {
              "srcdest": {
                "type": "string",
                "index": "not_analyzed",
                "doc_values": true,
                "omit_norms" : true
              },
              "dest": {
                "type": "string",
                "index": "not_analyzed",
                "doc_values": true,
                "omit_norms" : true
              },
              "src": {
                "type": "string",
                "index": "not_analyzed",
                "doc_values": true,
                "omit_norms" : true
              },
              "coordinates": {
                "type": "geo_point",
                "doc_values": true
              }
            }
          },
          "meta": {
            "properties": {
              "related": {
                "type": "string"
              },
              "char": {
                "type": "string",
                "index": "not_analyzed",
                "doc_values": true,
                "omit_norms" : true
              },
              "user": {
                "properties": {
                  "firstname": {
                    "type": "string",
                    "doc_values": true,
                    "omit_norms" : true
                  },
                  "lastname": {
                    "type": "integer",
                    "index": "not_analyzed",
                    "doc_values": true,
                    "omit_norms" : true
                  }
                }
              }
            }
          },
          "ip" : {
            "type" : "ip",
            "doc_values": true
          },
          "agent": {
            "type": "multi_field",
            "fields": {
              "agent": {
                "type": "string",
                "index": "analyzed",
                "omit_norms" : true
              },
              "raw": {
                "type": "string",
                "index": "not_analyzed",
                "doc_values": true,
                "omit_norms" : true
              }
            }
          },
          "request": {
            "type": "multi_field",
            "fields": {
              "request": {
                "type": "string",
                "index": "analyzed",
                "omit_norms" : true
              },
              "raw": {
                "type": "string",
                "index": "not_analyzed",
                "doc_values": true,
                "omit_norms" : true
              }
            }
          },
          "text" : {
            "index" : "analyzed",
            "type" : "string",
            "omit_norms" : true
          },
          "@message": {
            "type": "string",
            "index": "no"
          }
        },
        "_all" : {
          "enabled" : false
        }
      }
    },
    "aliases" : { },
    "order": 0
}